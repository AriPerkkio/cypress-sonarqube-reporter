{
    "name": "cypress-sonarqube-reporter",
    "version": "1.2.1",
    "description": "A SonarQube XML reporter for Cypress",
    "main": "index.js",
    "scripts": {
        "clean": "rimraf dist",
        "test": "npm run clean && jest --forceExit test/specs/",
        "lint": "eslint --format json --output-file dist/eslint-report.json src/** test/specs/**",
        "sonar": "node sonar-project.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BBE78/cypress-sonarqube-reporter.git"
    },
    "keywords": [
        "cypress",
        "sonarqube",
        "reporter",
        "mocha"
    ],
    "author": "Beno√Æt Berthonneau",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/BBE78/cypress-sonarqube-reporter/issues"
    },
    "homepage": "https://github.com/BBE78/cypress-sonarqube-reporter#readme",
    "dependencies": {
        "colors": "^1.4.0",
        "fs-extra": "^8.1.0",
        "mocha": "^6.2.2",
        "xmlbuilder": "^13.0.2"
    },
    "devDependencies": {
        "cypress": "^3.7.0",
        "cypress-multi-reporters": "^1.2.3",
        "eslint": "^6.7.1",
        "eslint-plugin-cypress": "^2.7.0",
        "eslint-plugin-jest": "^23.1.1",
        "fast-xml-parser": "^3.15.0",
        "jest": "^24.9.0",
        "jest-extended": "^0.11.2",
        "mochawesome": "^4.1.0",
        "rimraf": "^3.0.0"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "jest-extended"
        ],
        "testMatch": [
            "test/specs/**/*.[jt]s?(x)",
            "**/?(*.)+(spec).[jt]s?(x)"
        ]
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:cypress/recommended",
            "plugin:jest/recommended"
        ],
        "plugins": [
            "cypress",
            "jest"
        ],
        "parserOptions": {
            "ecmaVersion": 6
        },
        "env": {
            "cypress/globals": true,
            "jest/globals": true,
            "node": true
        }
    }
}
